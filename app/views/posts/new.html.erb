<%= form_for @post, html: { multipart: true} do |f| %>
	<%= f.label :title %>
	<%= f.text_field :title %>

	<%= f.label :tag_list, 'Porn Tags' %>
	<%= f.text_field :tag_list %>	

	<%= f.label :address %>
	<%= f.text_field :address %>	

	<%= f.label :picture %>
	<%= f.file_field :picture %>	

	<%= f.submit 'Create New Food Porn' %>
<% end %>